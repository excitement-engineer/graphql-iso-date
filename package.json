{
  "name": "graphql-iso-date",
  "version": "1.0.0",
  "description": "GraphQL scalar type that represents a date in the ISO format YYYY-MM-DD.",
  "main": "dist/index.js",
  "homepage": "https://github.com/excitement-engineer/graphql-iso-date",
  "bugs": {
    "url": "https://github.com/excitement-engineer/graphql-iso-date/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/excitement-engineer/graphql-iso-date.git"
  },
  "scripts": {
    "examples": "npm run build && babel-node examples/index.js",
    "build": "babel src --ignore __tests__ --out-dir dist/",
    "test": "mocha --require resources/mochaSetup src/**/__tests__/**/*.js",
    "check": "flow check",
    "prepublish": "npm run build",
    "travis": "npm run test && npm run check && npm run examples"
  },
  "keywords": [
    "GraphQL",
    "Scalar",
    "Date",
    "Node",
    "ISO"
  ],
  "author": "Dirk-Jan Rutten <dirkjanrutten@gmail.com>",
  "license": "MIT",
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "graphql": "^0.6.0",
    "moment": "^2.14.1"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "flow-bin": "^0.27.0",
    "mocha": "^2.5.3"
  }
}
